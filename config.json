{
	"title": "New Life Baptist Church",
	"baseURL": "http://newlifeconcord.org",
	"defaultExtension": "html",
	"uglyURLs": true,
	"canonifyurls": false,
	"relativeURLs": false,
	"enableRobotsTXT": false,
	"disableHugoGeneratorInject": true,
